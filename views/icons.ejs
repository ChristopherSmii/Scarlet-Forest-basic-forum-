<%-include('./navbar.ejs')%>
<body>
    <div class="container bg-dark my-4">
        <div class="row p-1">
            <%for(let i=0;i<icons.rows.length;i++){%>
                <div class="col-md-2 col-sm-3 col-6 p-1">
                    <a class="card" href="/target-icon/<%=icons.rows[i].icon_id%>">
                        <img src="<%=icons.rows[i].icon_path%>" alt="icon" class="card-img">
                        <div class="card-footer"><%=icons.rows[i].icon_name%></div>
                    </a>
                </div>
            <%}%>
        </div>
    </div>
</body>